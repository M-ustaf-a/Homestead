<% if(success && success.length){%>
    <div class="alert alert-success alert-dismissible fade show col-6 offset-3 mt-3" role="alert"><button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button><%=success%></div>
<% } %>
<% if(error && error.length){%>
  <div class="alert alert-danger alert-dismissible fade show col-6 offset-3 mt-3" role="alert"><button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button><%=error%></div>
<%}%>